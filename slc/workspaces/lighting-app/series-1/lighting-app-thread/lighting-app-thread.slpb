---
parameters:
- name: "build_dir"
constants:
- name: "project_name"
  value: "lighting-app-thread"
steps:
- task: "copy"
  output: "artifact/{{project_name}}.s37"
  export: "application_binary"
  input: "{{build_dir}}/{{project_name}}.s37"
- task: "usage"
  input: "{{build_dir}}/{{project_name}}.out"
  map: "{{build_dir}}/{{project_name}}.map"
