defines =
framework_dirs =
include_dirs = -Igen/include
cflags = -O0 -g2 -fno-common -ffunction-sections -fdata-sections -fno-exceptions -fno-unwind-tables -fno-asynchronous-unwind-tables -fPIC -Wall -Werror -Wextra -Wshadow -Wunreachable-code -Wvla -Wformat -Wformat-nonliteral -Wformat-security -Wno-deprecated-declarations -Wno-missing-field-initializers -Wno-unknown-warning-option -Wno-unused-parameter -Wno-cast-function-type -Wno-psabi -Wno-maybe-uninitialized -fdiagnostics-color -fno-strict-aliasing -I/usr/include/gio-unix-2.0 -I/usr/include/libmount -I/usr/include/blkid -I/usr/include/glib-2.0 -I/usr/lib/x86_64-linux-gnu/glib-2.0/include
cflags_c = -std=gnu11
target_output_name = bluez

build obj/src/messaging/tests/out/debug/gen/include/platform/Linux/dbus/bluez/bluez.DbusBluez.c.o: cc gen/include/platform/Linux/dbus/bluez/DbusBluez.c || obj/src/platform/Linux/dbus/bluez/bluez_gen.stamp

build obj/src/platform/Linux/dbus/bluez/lib/bluez.a: alink obj/src/messaging/tests/out/debug/gen/include/platform/Linux/dbus/bluez/bluez.DbusBluez.c.o || obj/src/platform/Linux/dbus/bluez/bluez_gen.stamp
  arflags =
  output_extension = .a
  output_dir = obj/src/platform/Linux/dbus/bluez/lib
