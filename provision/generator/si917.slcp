  - { name: SL_PROVISION_CHANNEL_ENABLED, value: '1' }
  - { name: SILABS_PROVISION_PROTOCOL_V1, value: '0' }
  - { name: CHIP_SYSTEM_CONFIG_USE_SOCKETS, value: '1' }
  - { name: OPTIMIZE_TINYCRYPT_ASM }
  - { name: TINYCRYPT_PRIMITIVES }
